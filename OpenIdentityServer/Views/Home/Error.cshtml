@{

    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class='container page-error' >
    <div class="page-header">
        <h1>Error</h1>
    </div>
    <div class="row">
        <div class="col-md-6 col-sm-6">
            <div class="alert alert-danger">
                <div ng-show="model.errorMessage">@ViewBag.ErrorMessabe</div>
            </div>
            <div class="request-id">Request Id: <span >>@ViewBag.RequestId</span></div>
        </div>
    </div>
</div>